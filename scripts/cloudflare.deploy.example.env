# Cloudflare API credentials (required for wrangler pages deploy)
CLOUDFLARE_ACCOUNT_ID=00000000000000000000000000000000
CLOUDFLARE_API_TOKEN=cf_example_token_with_pages_scope

# Cloudflare Pages project configuration
CF_PAGES_PROJECT=templ-frontend
CF_PAGES_BRANCH=production

# Frontend build configuration (Vite)
VITE_BACKEND_URL=https://api.templ.example
VITE_BACKEND_SERVER_ID=templ-prod
VITE_TEMPL_FACTORY_ADDRESS=0xYourFactory
VITE_TEMPL_FACTORY_DEPLOYMENT_BLOCK=12345678
VITE_TEMPL_FACTORY_PROTOCOL_RECIPIENT=0xProtocolTreasury
VITE_TEMPL_FACTORY_PROTOCOL_PERCENT=10
VITE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/your-key

# Optional: extra environment variables for the Vite build
# FRONTEND_BUILD_VAR_SOME_KEY=value
